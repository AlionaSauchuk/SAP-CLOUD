{
 "welcomeFile": "/api/",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "/api/(.*)",
      "target": "$1",
      "destination": "javarest",
      "authenticationType": "xsuaa"
    },
    {
	  "source": "/s4c/(.*)",
	  "target": "/$1",
	  "destination": "S4C",
      "authenticationType": "xsuaa"
  	}
  ]
}
