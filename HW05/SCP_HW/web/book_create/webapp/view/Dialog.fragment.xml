<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Select Book"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{books>/BooksCollection}"
	>
		<StandardListItem
			press="onSelect"
			title="{books>caption}"
			description="{books>bid}"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>